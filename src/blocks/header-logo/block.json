{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "np/header-logo",
	"version": "1.0.0",
	"title": "Header Logo",
	"category": "",
	"icon": "layout",
	"description": "The logo of the site with cool effects. For use within the Header only.",
	"keywords": ["header", "logo", "image"],
	"parent": ["np/header"],
	"supports": {
		"html": false
	},
	"attributes": {
		"disableHover": {
			"type": "boolean",
			"default": false
		},
		"backgroundColor": {
			"type": "string",
			"default": ""
		},
		"logoColor": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "np-header-logo",
	"editorStyle": "file:./index.css",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}